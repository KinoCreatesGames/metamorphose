        {
            "name": "dialogue",
            "description": "Dialogue entries for the game",
            "displayColumn": "id",
            "guid": "69f2e8e0-81bc-49a5-817a-80a346c6369a",
            "columns": [
                {
                    "typeStr": "text",
                    "guid": "1aa8a958-20d2-4eb5-89ce-53ac0dc0fc03",
                    "name": "name",
                    "description": "The name of the dialogue scene within the game",
                    "defaultValue": "Cutscene",
                    "iconName": "newText",
                    "configurable": {
